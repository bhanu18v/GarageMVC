@model GarageApp_2._0.Models.ViewModel.Overview

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>


<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                Registration Number
            </th>
            <th>
                Vehicle Type
            </th>
            <th>
                Color
            </th>
            <th>
                Parked Time
            </th>
            <th>View details of vehicle or Unpark Vehicle</th>
        </tr>
    </thead>
    <tbody>

                @foreach (var item in Model)
        @*foreach(Feedback item in (System.Collections.Generic.IEnumerable<Feedback>)ViewData.Model)
                            {*@{ 
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.RegNr)</td>
                    <td>@Html.DisplayFor(modelItem => item.Type)</td>
                    <td>@Html.DisplayFor(modelItem => item.Color)</td>
                    <td>@Html.DisplayFor(modelItem => item.ParkedTime)</td>
                    <td>
                        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                    @Html.ActionLink("Check Out", "Receipt", new { id = item.Id })
                </td>
            </tr>
            }
</tbody>
</table>
